{
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "socialruins/raptoreum-php",
        "version": "0.1",
        "source": {
          "url": "https://github.com/socialruins/raptoreum-php.git",
          "type": "git",
          "reference": "main"
        }
      }
    }
  ],
  "name": "socialruins/raptoreum-p2p",
  "description": "Implementation of the raptoreum protocol using ReactPHP",
  "type": "library",
  "homepage": "https://github.com/bit-wasp/node-php",
  "license": "Unlicense",
  "authors": [
    {
      "name": "Thomas Kerin",
      "homepage": "https://thomaskerin.io",
      "role": "Author"
    }
  ],
  "autoload": {
    "psr-4": {
      "Raptorio\\Blockchain\\Networking\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BitWasp\\Bitcoin\\Tests\\Networking\\": "tests/unit/"
    }
  },
  "require": {
    "socialruins/raptoreum-php": "^0.1",
    "react/socket": "^0.8.0",
    "react/dns": "^0.4.0",
    "christian-riesen/base32": "^1.3.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.0",
    "phpunit/phpunit": "^6.0",
    "symfony/yaml": "~2.6",
    "phpstan/phpstan": "^0.9.2"
  }
}
